//Static methods and variables 

class Num {

	int i;
	static int s;
	
	static void FromStatic()
	{
		System.out.println(s);
		//System.out.println(i);
	}
}
public class CH31 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Num v1=new Num();
		v1.i=100;
		v1.s=900;
		
		Num v2=new Num();
		v2.i=200;
		v2.s = 500;  //v1.s will be equal to 500 because s is static and has static value assigned currently
		
		System.out.println("v1.i "+v1.i);
		System.out.println("v1.s " + v1.s);
		
		System.out.println("v2.i "+v2.i);
		System.out.println("v2.s " + v2.s);
		
		System.out.println(Num.s);
		
		Num.FromStatic(); //static method can be called using class.method() and without creating object

	}

}
