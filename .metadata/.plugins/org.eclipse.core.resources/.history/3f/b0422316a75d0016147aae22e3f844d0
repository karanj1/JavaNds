package ds.queue;

public class Queue {
	
	private int maxSize;
	private long[] queArray;
	private int front;
	private int rear;
	private int nItems;
	
	public Queue(int size){
		maxSize = size;
		queArray = new long[maxSize];
		front = 0;
		rear = -1;
		rear = 0;
	}
	
	public void insert(long j){
		rear++;
		queArray[rear] = j;
		nItems++;
	}
	
	public void view(){
		queArray[front];
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
