
public class CH31 {

	int i;
	static int s;
	
	static void FromStatic()
	{
		System.out.println(s);
		//System.out.println(i);
	}
}

public class StaticAgain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		CH31 v1=new CH31();
		v1.i=100;
		v1.s=900;
		
		CH31 v2=new CH31();
		v2.i=200;
		
		System.out.println("v1.i "+v1.i);
		System.out.println("v1.s " + v1.s);
		
		System.out.println("v2.i "+v2.i);
		System.out.println("v2.s " + v2.s);
		
		System.out.println(CH31.s);

	}