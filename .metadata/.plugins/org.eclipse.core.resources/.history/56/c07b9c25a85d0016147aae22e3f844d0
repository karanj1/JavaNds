package ds.queue;

public class Queue {
	
	private int maxSize;
	private long[] queArray;
	private int front;
	private int rear;
	private int nItems;
	
	public Queue(int size){
		maxSize = size;
		queArray = new long[maxSize];
		front = 0;
		rear = -1;
		nItems = 0;
	}
	
	public void insert(long j){
		rear++;
		queArray[rear] = j;
		nItems++;
	}
	
	public void view(){
		System.out.print("[ ");
		for(int i=0; i < queArray.length; i++){
			System.out.print(queArray[i]+ " ");
		}
		System.out.println("]");
	}
	
	public long remove(){
		long temp = queArray[front++];
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Queue myQueue = new Queue(5);
		myQueue.insert(100);
		myQueue.insert(200);
		myQueue.insert(300);
		myQueue.insert(400);
		myQueue.insert(500);
		
		myQueue.view();
	}

}
